    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach (Remote Debug Docker)",
            "type": "python",
            "request": "attach",
            "port": 5678,
            "host": "localhost"
        },
        {
            "name": "FrameworkLive Sandbox Neo4j-local",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/FrameworkLive.py",
            "console": "integratedTerminal",
            "args": [
                "--noplot",
                "--resultsdir=./results/",
                "--neo4jmode=local"
            ]
        },
        {
            "name": "FrameworkLive Sandbox Neo4j-AWS",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/FrameworkLive.py",
            "console": "integratedTerminal",
            "args": [
                "--noplot",
                "--resultsdir=./results/",
                "--neo4jmode=aws"
            ]
        },

        {
            "name": "FrameworkLive NO Sandbox",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/FrameworkLive.py",
            "console": "integratedTerminal",
            "args": [
                "--noplot",
                "--resultsdir=./results/",
                "--live"
            ]
        },
        {
            "name": "FrameworkSimDB",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/FrameworkSimDB.py",
            "console": "integratedTerminal",
            "args": [
                "--limit=10",
                "--resultsdir=./results/"
            ]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
        }
    ]
}