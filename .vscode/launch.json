    "version": "0.2.0",
    "configurations": [
        {
            "name": "FrameworkLive",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/FrameworkLive.py",
            "console": "integratedTerminal",
            "args": [
                "--noplot",
                "--resultsdir=./results/"
            ]
        },
        {
            "name": "FrameworkSimDB",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/FrameworkSimDB.py",
            "console": "integratedTerminal",
            "args": [
                "--limit=10",
                "--resultsdir=./results/"
            ]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
        }
    ]
}